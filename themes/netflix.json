{
    "$schema":"https://example.com/hospital-theme.schema.json",
    "name":"netflix-impact","version":"1.0.0",
    "meta":{ "description":"Bold campaign visuals for health promotion","brandTone":["bold","high-contrast","impact"],"fontsNote":"Archivo (Headings), Roboto (Body)" },
    "primitives":{
      "colors":{
        "white":"#FFFFFF","black":"#111111",
        "gray":{"50":"#F7F7F7","100":"#EBEBEB","200":"#DCDCDC","300":"#C4C4C4","400":"#A3A3A3","500":"#7A7A7A","600":"#555","700":"#3C3C3C","800":"#222","900":"#171717"},
        "red":{"50":"#FFE9EA","100":"#FFCBCD","200":"#FF9A9F","300":"#FF6A72","400":"#F43C45","500":"#E50914","600":"#C40812","700":"#A10710","800":"#7F050C","900":"#5E0409"},
        "accent":{"blue":"#3B82F6"}
      },
      "font":{
        "families":{"heading":"'Archivo', ui-sans-serif, system-ui, -apple-system, 'Segoe UI', Roboto, Arial","body":"'Roboto', ui-sans-serif, system-ui, -apple-system, 'Segoe UI', Arial","mono":"ui-monospace, Menlo, Consolas, monospace"},
        "sizes":{"xs":"0.75rem","sm":"0.875rem","base":"1rem","lg":"1.125rem","xl":"1.375rem","2xl":"1.75rem","3xl":"2.125rem","4xl":"2.5rem"},
        "weights":{"regular":400,"medium":500,"semibold":600,"bold":800},
        "lineHeights":{"tight":1.05,"snug":1.2,"normal":1.5,"relaxed":1.65}
      },
      "radii":{"xs":"3px","sm":"6px","md":"8px","lg":"12px","xl":"18px","pill":"9999px"},
      "spacing":{"0":"0px","1":"4px","2":"8px","3":"12px","4":"16px","5":"20px","6":"24px","8":"32px","10":"40px","12":"48px","16":"64px"},
      "shadows":{"sm":"0 1px 2px rgba(0,0,0,0.2)","md":"0 6px 16px rgba(0,0,0,0.28)","lg":"0 12px 30px rgba(0,0,0,0.35)","focusRing":"0 0 0 3px rgba(229,9,20,0.45)"},
      "breakpoints":{"sm":"640px","md":"768px","lg":"1024px","xl":"1280px","2xl":"1536px"}
    },
    "semantic":{
      "bg":{"base":"{primitives.colors.black}","subtle":"#161616","elevated":"#1D1D1D"},
      "fg":{"primary":"{primitives.colors.white}","secondary":"{primitives.colors.gray.300}","muted":"{primitives.colors.gray.500}","onPrimary":"{primitives.colors.white}"},
      "border":{"subtle":"#2A2A2A","strong":"#3A3A3A","focus":"{primitives.colors.red.400}"},
      "primary":{"default":"{primitives.colors.red.500}","hover":"{primitives.colors.red.400}","active":"{primitives.colors.red.600}","mutedBg":"#2A0C0E","mutedFg":"{primitives.colors.red.400}"},
      "success":{"default":"#22C55E","mutedBg":"rgba(34,197,94,0.12)","mutedFg":"#22C55E"},
      "warning":{"default":"#F59E0B","mutedBg":"rgba(245,158,11,0.12)","mutedFg":"#F59E0B"},
      "danger":{"default":"{primitives.colors.red.500}","mutedBg":"rgba(229,9,20,0.12)","mutedFg":"{primitives.colors.red.400}"},
      "elevation":{"card":"{primitives.shadows.md}","modal":"{primitives.shadows.lg}","popover":"{primitives.shadows.md}"}
    },
    "components":{
      "button":{"radius":"{primitives.radii.sm}","height":"44px","paddingX":"18px","font":{"family":"{primitives.font.families.heading}","size":"{primitives.font.sizes.base}","weight":"{primitives.font.weights.bold}"},
        "variants":{"primary":{"bg":"{semantic.primary.default}","fg":"{semantic.fg.onPrimary}","hoverBg":"{semantic.primary.hover}","activeBg":"{semantic.primary.active}","focusRing":"{primitives.shadows.focusRing}"},
          "secondary":{"bg":"#2A2A2A","fg":"{semantic.fg.primary}","border":"#3A3A3A","hoverBg":"#343434"},
          "ghost":{"bg":"transparent","fg":"{semantic.primary.default}","hoverBg":"{semantic.primary.mutedBg}"}
        }
      },
      "input":{"radius":"{primitives.radii.sm}","height":"44px","paddingX":"14px","bg":"#1F1F1F","fg":"{semantic.fg.primary}","placeholder":"{primitives.colors.gray.500}","border":"#2A2A2A","focusBorder":"{semantic.border.focus}","focusRing":"{primitives.shadows.focusRing}"},
      "card":{"radius":"{primitives.radii.lg}","bg":"{semantic.bg.elevated}","fg":"{semantic.fg.primary}","shadow":"{semantic.elevation.card}","border":"#2A2A2A"}
    },
    "dataViz":{"palette":["#E50914","#3B82F6","#22C55E","#F59E0B","#A855F7","#EF4444"],"positive":"#22C55E","neutral":"#A3A3A3","negative":"#E50914"}
  }
  